

@{
    ViewBag.Title = "Atendiendo";
}

@section customStyles {
    <link rel="stylesheet" href="~/Content/Estilos-AtenderConsultas/Atendiendo.css" />
}

<div class="row">
    @* Numero de expediente card *@
    <div class="col-3 p-4  Expediente">
        <div class="border shadow-sm rounded-3 p-2 d-flex flex-column justify-content-center align-items-center" style="height: 320px;" >
            <h5 class="my-3" >Expediente Nro.</h5>
            <p id="HistoriaClinica"></p>
            <svg width="143" height="157" viewBox="0 0 143 157" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                <ellipse cx="71.5822" cy="78.387" rx="71.0127" ry="77.7226" fill="url(#pattern0_229_9829)" />
                <defs>
                    <pattern id="pattern0_229_9829" patternContentUnits="objectBoundingBox" width="1" height="1">
                        <use xlink:href="#image0_229_9829" transform="matrix(0.00213767 0 0 0.00195312 -0.0472447 0)" />
                    </pattern>
                    <image id="image0_229_9829" width="512" height="512" xlink:href="data:image/png;base64," />

                </defs>
            </svg>
        </div>
    </div>

    <div class="links-nav  col-9">
        <nav class="navbar">
            <div class="container-fluid">
                <ul class="navbar-nav ms-auto d-flex flex-row">
                    <li class="nav-item mx-4">
                        <a class="nav-link fw-bold active" data-content="Portadas" href="#">Portadas</a>
                    </li>
                    <li class="nav-item mx-4">
                        <a class="nav-link fw-bold" data-content="Citas" href="#">Citas</a>
                    </li>
                    <li class="nav-item mx-4">
                        <a class="nav-link fw-bold" data-content="Recetas" href="#">Recetas</a>
                    </li>
                </ul>
            </div>
        </nav>
        <div id="navbar-content-top"></div>
    </div>
</div>
<hr />
<div class="row">
    <div class="col-3 p-4 pt-5 ">
        <div class="border p-3 mt-5 rounded-3 shadow-sm">
            <h4 class="mb-3 text-center">Consulta</h4>
            <input id="CodigoConsultaInputMain" type="hidden" name="name" value="" />
            <div>
                <label>Fecha:</label>
                <p class="ms-3" id="fechaCita"></p>
            </div>
            <div>
                <label>Hora:</label>
                <p class="ms-3" id="Hora"> tipo</p>
            </div>
            <div>
                <label>Responsable:</label>
                <p class="ms-3" id="medico"> dr. Roque xd</p>
            </div>
            <div>
                <label>Codigo:</label>
                <p class="ms-3" id="CodigoCita"> No hay</p>
            </div>
        </div>
    </div>

    <div class="links-nav  col-9">
        <nav class="navbar">
            <div class="container-fluid">
                <ul class="navbar-nav ms-auto d-flex flex-row">
                    <li class="nav-item mx-4">
                        <a class="nav-link fw-bold active" data-content="Inicio" href="#">Inicio</a>
                    </li>
                    <li class="nav-item mx-4">
                        <a class="nav-link fw-bold" data-content="Diagnosticos" href="#">Diagnosticos</a>
                    </li>
                    <li class="nav-item mx-4">
                        <a class="nav-link fw-bold" data-content="Tratamiento" href="#">Tratamiento</a>
                    </li>
                </ul>
            </div>
        </nav>
        <div id="navbar-content-bottom"></div>

    </div>
</div>


<!-- Modal de confirmación -->
<div class="modal fade" id="modalConfirmacion" tabindex="-1" aria-labelledby="modalConfirmacionLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="modalConfirmacionLabel">Confirmación</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                ¿Está seguro de que desea finalizar la consulta?
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancelar</button>
                <button type="button" id="btnConfirmarFinalizar" class="btn btn-success" data-bs-dismiss="modal">Finalizar</button>
                
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/Scripts/AtenderConsultasScripts/Atendiendo.js"></script>
    <script src="~/Scripts/AtenderConsultasScripts/llenarDatosPaciente.js"></script>


    <script>
        document.getElementById("btnConfirmarFinalizar").addEventListener("click", function () {
            var citaCodigo = document.getElementById("CodigoCita").textContent.trim();

            var consultaCodigo = document.getElementById("CodigoConsultaInput").value.trim();

            $.ajax({
                type: "POST",
                url: '@Url.Action("ActualizarEstadoFinalizadoConsulta", "AtenderConsultas")',
                data: { citaCodigo: citaCodigo },
                success: function (response) {
                    //alert(response.mensaje);
                    window.location.href = '@Url.Action("Consultas", "AtenderConsultas")';
                },
                error: function (xhr, status, error) {
                    alert("Hubo un problema al finalizar la consulta.");
                }
            });

            $.ajax({
                type: "POST",
                url: '@Url.Action("ActualizarHoraFinalConsulta", "AtenderConsultas")',
                data: { codigoConsulta: consultaCodigo },
                success: function (response) {
                    //alert(response.mensaje);
                },
                error: function (xhr, status, error) {
                    alert("Hubo un problema al actualizar la fecha y hora final.");
                }
            });
        });
    </script>
}
